@model IEnumerable<NhaKhoa.Models.DonThuoc>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/NhaSi/Views/Shared/Layout.cshtml";
}


<table class="table">
    <tr>
        <th>
           Tên bệnh nhân
        </th>
        <th>
         Chẩn đoán
        </th>
        <th>
           Ngày khám
        </th>
        <th>
           Tổng số lượng
        </th>
        <th>
           Tổng Tiền 
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PhieuDatLich.AspNetUser.FullName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Chandoan)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NgayGio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Soluong)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TongTien)
        </td>
        <td>
            @Html.ActionLink("Chi tiết", "Details", new { id=item.Id_donthuoc })
        </td>
    </tr>
}

</table>
